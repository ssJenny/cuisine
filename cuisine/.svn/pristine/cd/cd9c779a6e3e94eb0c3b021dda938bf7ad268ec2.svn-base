	width: 1240px;
	margin: 0 auto;
}
.app_images .imgs_items{
	margin-top: 20px;
    margin-right: 30px;	
    width: 280px;
    height: 295px;
    overflow: hidden;
    float: left;
    position: relative;
}
.app_images .imgs_items p{
	color: #FF6537;
	font-weight: bold;
	font-size: 22px;
	position: absolute;
	bottom: 11px;
	right: 5px;
	text-align: center;
	display: none;
	z-index: 10;
}
.app_images .imgs_items img{
	width: 100%;
	height: 100%;
	transition: all 0.5s ease 0s;
}
.app_images .imgs_items:hover p{
	display: block;
	transition: all .5s ease 0s;
}
.app_images .imgs_items:hover img{
	transform: scale(1.2);
}
.gaishu_goleft,
.gaishu_goright {
    width: 58px;
    height: 58px;
    background: url(../img/aboutxiaoniao/goleft.png) no-repeat;
    position: absolute;
    top: 50%;
    margin-top: -29px;
    cursor: pointer;
}
.gaishu_goright{
	background: url(../img/aboutxiaoniao/goright.png) -58px 0 no-repeat;
    right: 60px;
}
.gaishu_goleft{
