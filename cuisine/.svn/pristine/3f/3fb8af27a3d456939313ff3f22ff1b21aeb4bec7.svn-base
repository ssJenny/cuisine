.header_wrap{
	background: #221236 url(../img/aboutxiaoniao/beader_bg.jpg) top center;
	min-width: 1100px;
	overflow: hidden;
}
.header{
	width: 1100px;
	margin: 0 auto;
	height: 50px;
}
.logo{
	float: left;	
}
.nav_wrap{
	float: right;
	width: 930px;
	height: 39px;
	line-height: 39px;
	padding-top: 11px;
	text-align: right;
}
.nav_wrap>li{
	float: left;
	color: #fff;
	position: relative;
}
.nav_wrap h1{
	font-size: 16px;
	text-align: center;
	padding: 0 15px;
	position: relative;
	cursor: pointer;
	height: 30px;
	color: #aeabb4;
}
.nav_wrap h1:hover{
	background: url(../img/nav_now_bg.png) bottom center no-repeat;
	color: #fff;
	transition: all .5s ease 0s; 
}

.welcome_wrap{
	width: 100%;
	background: #299ad6;
	position: fixed;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
	/*display: none;*/
	padding-top: 50px;
	z-index: 999;
}
.welcome_content{
	height: 560px;
	position: absolute;
	top: 50%;
	margin-top: -280px;
	text-align: center;
	width: 100%;
}
.xiaoniao{
	text-align: center;
}
.welcome_text_container{
	margin-top: 10px;
	text-align: center;
	color: #fff;
	display: ;
}
.welcome_animate{
	display: none;
}
.welcome_text_container .title{
	font-size: 44px;
	line-height: 48px;
	
}
.welcome_text_container .en_title{
	font-size: 16px;
	font-family: arial, helvetica, sans-serif;
	letter-spacing: 4px;
	text-transform: capitalize;
}
.welcome_text_container .ems{
	padding-top: 15px;
}
.welcome_text_container .ems span{
	font-size: 48px;
}
.yinhaozuo{
	position: relative;
	left: -8px;
	top: -30px;
}
.yinhaoyou{
	position: relative;
	right: -8px;
	top: 17px;
}
.welcome_text_container p{
	line-height: 30px;
	font-size: 20px;
}
.main_wrap{
	width: 100%;
	min-width: 1100px;
	background: #fff;
	overflow: hidden;
	position: relative;
}
.container{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
}
.wrap_block{
	width: 100%;
	position: relative;
}
.welcome_wrap1{
	background: #221236;
	
}

.welcome_content1 {
    /*height: 560px;*/
     position: absolute; 
    top: 40%;
     margin-top: -280px; 
    text-align: center;
    width: 100%;
}
.donext{
	height: 58px;
	width: 58px;
	background:  url(../img/aboutxiaoniao/donext.png) 0 -58px no-repeat;
	margin:20px auto 0;
	cursor: pointer;
}
.donext:hover {
    background-position: 0 0;
    transition: all 0.5s ease 0s;
}
.gaishu_slider{
	width: 6000px;
	position: absolute;
	left: 0;
	top: 0;
}
.gaishu_block {
    background: #fff;
    float: left;
    width: 1100px;
    height: 560px;
    margin-bottom: 500px;
    margin: 0 auto;
    position: relative;
}
.gaishu_block_content{
	width: 100%;
	height: 560px;
	position: absolute;
	top: 45%;
    margin-top: -280px;
	text-align: center;
}
.gaishu_block_content>h1{
	font-size: 54px;
    color: #311759;
    font-weight: bold;
}
.gaishu_block_content .ems{
	font-size: 18px;
    color: #6b6a6f;
    width: 630px;
    line-height: 30px;
    margin: 0 auto;
    position: relative;
    margin-top: 20px;
}
.ems .yinhao_zuo{
	position: absolute;
    left: -34px;
    top: -3px;
}
.ems .yinhao_you{
	position: absolute;
    right: -30px;
    top: 64px;
}
.app_images img{
	margin-top: 20px;
    margin-right: 30px;	
}

.gaishu_goleft,
.gaishu_goright {
    width: 58px;
    height: 58px;
    background: url(../img/aboutxiaoniao/goleft.png) no-repeat;
    position: absolute;
    top: 50%;
    margin-top: -29px;
    cursor: pointer;
}
.gaishu_goright{
	background: url(../img/aboutxiaoniao/goright.png) -58px 0 no-repeat;
    right: 60px;
}
.gaishu_goleft{
	left: 60px;
}
.gaishu_goleft:hover {
    background-position: -58px 0;
    transition: all 0.5s ease 0s;
}
.gaishu_goright:hover {
    background-position: 0 0;
    transition: all 0.5s ease 0s;
}
.gaishu_block_content .block{
	margin-top: 50px;
    overflow: hidden;
    color: #6b6a6f;
    text-align: left;
}
.gaishu_block_content .block>*{
	float: left;
}
.gs_block_2{
	width: 700px;
    left: 50%;
    margin-left: -350px;
    top: 50%;
}
.gs_block_2 h1{
	font-size: 24px;
    font-weight: normal;
    margin-top: 20px;
}
.gs_block_2 h2{
	font-size: 18px;
    font-weight: normal;
}
.gs_block_2 .text{
	width: 520px;
    margin-left: 55px;
}
.gs3_block .ems{
	color: #221236;
    font-size: 36px;
}
.gaishu_block_content .ems .yinhao3_zuo {
    position: relative;
    left: -8px;
    top: -30px;
}
.gaishu_block_content .ems .yinhao3_you {
    position: relative;
    right: -10px;
    top: 18px;
}
.gs3_block p {
    width: 730px;
    margin: 55px auto 0;
    line-height: 30px;
    font-size: 18px;
    color: #6b6a6f;
    text-align: left;
    text-indent: 2em;
}
.jiazhi_content{
	width: 970px;
	height: 560px;
	position: absolute;
	top: 50%;
	left: 50%;
	margin: -280px 0 0 -485px;
}
.jiazhi_content>h1{
	font-size: 48px;
	text-align: center;
	margin-bottom: 100px;
}
.jiazhi_top_corners,
.jiazhi_bottom_corners{
	overflow: hidden;
	margin: 10px 0;
}
.jiazhi_content .contents{
	height: 317px;
	width: 850px;
	margin: 0  auto;
	overflow: hidden;
}
.jiazhi_top_corners>div,
.jiazhi_bottom_corners>div{
	float: left;
	height: 20px;
	width: 20px;
	position: relative;
}
.jiazhi_top_corners img,
.jiazhi_bottom_corners img{
	position: absolute;
	left: 0;
	top: 0;
}
.jiazhi_top_corners>.corners_right,
.jiazhi_bottom_corners>.corners_right{
	float: right;
}
.jiazhi_content .contents>.contents_items{
	float: left;
	width: 390px;
	margin-right: 30px;
	margin-bottom: 46px;
}
.jiazhi_content .contents_items .number{
	width: 74px;
	height: 74px;
	background:  url(../img/aboutxiaoniao/jiazhi_quanquan.png) 0 0 no-repeat;
	line-height: 74px;
	text-align: center;
	margin-right: 27px;
	float: left;
	font-family: arial, helvetica, sans-serif;
}
.jiazhi_content .contents_items p{
	float: left;
	width: 280px;
	font-size: 18px;
	margin-top: 13px;
}
.text-white{
	color: #fff;
}
.breathLight{
	display: none;
}
.zhangyun{
	background: #f1f2f6;
}
.yun_content{
	width: 910px;
	height: 560px;
	position: absolute;
	top: 50%;
	left: 50%;
	margin: -280px 0 0 -455px;
}
.yun_content h1{
	font-size: 54px;
    margin-bottom: 20px;
    text-align: center;
    color: #311759;
}
.yun_slidebox{
	height: 420px;
	width: 910px;
	overflow: hidden;
	position: relative;
}
.yun_slide{
	width: 1820px;
	height: 420px;
	top: 0;
	left: 0;
}
.slide_items{
	width: 910px;
	float: left;
	line-height: 36px;
	background: url(../img/aboutxiaoniao/xuxian_bg.png);
	font-size: 16px;
	font-family: kartika;
	color: #535355;
}
