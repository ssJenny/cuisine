.bananer-bar {
	z-index: 20;
	position: absolute;
	width: 216px;
	height: 29px;
	left: 50%;
	transform: translate(-50%,0);
	bottom: 40px;
	background: url(../img/next&prev_line.png) no-repeat left center;
}
.bananer-bar span{
	display: inline-block;
	cursor: pointer;
}
.bananer-bar>.prev{
	left: 0;
}
.prev{
	width: 29px;
	height: 29px;
	position: absolute;	
	cursor: pointer;
	background: url(../img/prev_jiantou.png) no-repeat center;
}
.prev:hover{
	animation: su-pudding .8s;
	background: url(../img/prev_jiantou_hover.png) no-repeat center;
}
.bananer-bar>.next{
	right: 0;
	top: 0;
}
.next{
	cursor: pointer;
	width: 29px;
	height: 29px;
	position: absolute;	
	background: url(../img/next_jiantou.png) no-repeat center;
}
.next:hover{
	animation: su-pudding .8s;
	background: url(../img/next_jiantou_hover.png) no-repeat center;
}
.middle-points{
	display: block;
	width: 45px;
	height: 14px;
	margin: 0 auto;
	margin-top: 7px;
	background: url(../img/next&prev_mbg01.png) no-repeat;
}
.middle-wrap{
	height: 14px;
	background: url(../img/next&prev_mbg03.png) no-repeat right;
}
.middle-circle{
	height: 14px;
	margin: 0 6px;
	background: url(../img/next&prev_mbg02.png) repeat-x;
}
.middle-circle>span{
	display: block;
	width: 6px;
	height: 6px;
	margin-right: 7px;
	margin-top: 4px;
	float: left;
	background: url(../img/next&prev_others.png);
}
.middle-circle>span:last-child{
	margin-right: 0;
}
.middle-circle .now{
	background: url(../img/next&prev_now.png);
}
